{
  "name": "bloxtension",
  "displayName": "Bloxtension",
  "version": "0.0.1",
  "description": "A Blockscout-enabled tool for convenient, multi-chain address, contract and transaction lookups.",
  "author": "me@lufy.cz",
  "scripts": {
    "dev": "plasmo dev --target=firefox-mv3 --hmr-host=0.0.0.0 --hmr-port=1815",
    "build": "plasmo build --target=firefox-mv3",
    "package": "plasmo package"
  },
  "dependencies": {
    "@blockscout/app-sdk": "^0.1.0",
    "@plasmohq/storage": "^1.15.0",
    "@plasmohq/messaging": "^0.7.1",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "scripting",
      "contextMenus"
    ],
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "../src/contents/parse-ethereum-data.ts"
        ]
      }
    ]
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
